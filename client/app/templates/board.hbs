{{main-menu}}

<section class="row main_content">
	<h1 class="table_heading">Personal Board</h1>
	<table class="col-md-12 table main_table">
		<thead class="main_table_head">
			<tr class="row">
				<th class="col-md-4 chalk_border">
					Todo
				</th>
				<th class="col-md-4 chalk_border">
					Doing
				</th>
				<th class="col-md-4 chalk_border">
					Done
				</th>
			</tr>
		</thead>
		<tr class="row">
			<td class="col-md-4">
				<div class="td_ticket_wrapper">
					{{#each getTodo}}
						{{ticket-comp ticket=this}}
					{{/each}}
				</div>
			</td>
			<td class="col-md-4">
				<div class="td_ticket_wrapper">
					{{#each getDoing}}
						{{ticket-comp ticket=this}}
					{{/each}}
				</div>
			</td>
			<td class="col-md-4 done">
				<div class="td_ticket_wrapper">
					{{#each getDone}}
						{{ticket-comp ticket=this}}
					{{/each}}
				</div>
			</td>
		</tr>
	</table>
</section>